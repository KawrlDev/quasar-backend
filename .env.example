APP_NAME="MAMS"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_LEVEL=debug

# -------------------------
# Database
# -------------------------
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=mamsdb
DB_USERNAME=root
DB_PASSWORD=

# -------------------------
# Session / Sanctum
# -------------------------
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=
SESSION_SECURE_COOKIE=false
SESSION_SAME_SITE=lax
SESSION_EXPIRE_ON_CLOSE=false

SANCTUM_STATEFUL_DOMAINS=

# -------------------------
# Cache / Queue
# -------------------------
CACHE_STORE=file
QUEUE_CONNECTION=database

# -------------------------
# Files
# -------------------------
FILESYSTEM_DISK=local

# -------------------------
# Mail
# -------------------------
MAIL_MAILER=log
MAIL_HOST=
MAIL_PORT=
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=
MAIL_FROM_ADDRESS=
MAIL_FROM_NAME="${APP_NAME}"

# -------------------------
# Redis (optional)
# -------------------------
REDIS_CLIENT=
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=

# -------------------------
# Frontend (Vite / Quasar)
# -------------------------
VITE_APP_NAME="${APP_NAME}"
VITE_API_URL=